{
  "manifest_version": 3,
  "name": "Open Vite Project",
  "version": "1.0",
  "description": "A Chrome extension to open a Vite project in a new window",
  "permissions": ["tabs"],
  "action": {
    "default_title": "Open Vite Project",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
